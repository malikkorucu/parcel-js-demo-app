body,
html {
  background: #dedede;
  height: 100%;
}

.box a:hover {
  border: 1px solid #c26e6e;
}
.box:hover {
  border: 1px solid red;
}
.box::after {
  content: "asdfasdfasdfasdf";
  width: 20px;
  height: 20px;
  background-color: #c70f0f;
}

.clip-path {
  width: 200px;
  height: 200px;
  background-color: red;
  margin-top: 20px;
  clip-path: polygon(50% 0%, 80% 10%, 100% 35%, 100% 70%, 80% 90%, 50% 100%, 20% 90%, 0% 70%, 0% 35%, 20% 10%);
}
