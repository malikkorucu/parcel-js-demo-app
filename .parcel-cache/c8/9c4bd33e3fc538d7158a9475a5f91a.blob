body,
html {
  background: #dedede;
  height: 100%;
  font-family: "Poppins", sans-serif;
}

.box {
  background: url("data:image/jpeg;base64,");
  width: 400px;
  height: 400px;
  background-repeat: no-repeat;
}

.clip-path {
  width: 200px;
  height: 200px;
  background-color: red;
  margin-top: 20px;
  clip-path: polygon(28% 11%, 100% 0%, 100% 75%, 75% 75%, 75% 100%, 33% 36%, 0% 75%);
}